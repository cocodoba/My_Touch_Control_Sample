<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shinoharanaoki.mytouchcontrolsample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MoveImageViewActivity" />
        <activity android:name=".MoveBallObjectActivity" />
        <activity android:name=".ThreeBallsActivity" />
        <activity android:name=".LoopingBalloonsActivity" />
        <activity android:name=".FlashBallsActivity" />
        <activity android:name=".ChordBoardActivity"/>
    </application>

</manifest>